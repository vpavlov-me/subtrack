# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# WorkOS AuthKit (SSO)
VITE_WORKOS_CLIENT_ID=your-workos-client-id

# Stripe (Billing)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Stripe Webhook (Server-side only - use edge function proxy)
# STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# Slack Integration (Server-side only - use edge function proxy)
# SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-webhook-url

# Monitoring & Analytics (Optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@your-project.ingest.sentry.io/your-project-id
VITE_POSTHOG_KEY=phc_your-posthog-key
VITE_POSTHOG_HOST=https://app.posthog.com

# Vercel Environment (Auto-detected)
# VERCEL_URL=https://your-project.vercel.app
# VERCEL_ENV=production

# Development Only
# VITE_DEBUG_MODE=true
# VITE_API_MOCKING=enabled 